  "main": "server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "supervisor --watch server server/index.js",
    "dev:dashboard": "node build/dashboard.dev.js & cross-env SERVER_ALONE=alone supervisor --watch server server/index.js",
    "start": "cross-env NODE_ENV=production supervisor --watch server server/index.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.js --progress --hide-modules",
    "build:dashboard": "cross-env NODE_ENV=production webpack --config ./build/webpack.dashboard.js --progress --hide-modules",
    "build:dev": "webpack --config ./build/webpack.client.js & webpack --config ./build/webpack.server.js",
    "build": "npm run build:client & npm run build:server & npm run build:dashboard"
  },
  "repository": {
